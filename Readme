def digital_root(n):
    a = 0
    b = 0
    c = 0
    n = str(n)
    for i in n:
        i = int(i)
        a += i
    if a > 9:
        a = str(a)
        for j in a:
            j = int(j)
            b += j
        if b > 9:
            b = str(b)
            for k in b:
                k = int(k)
                c += k
            return c
        else:
            return b
    else:
        return a

Test.assert_equals(digital_root(16), 7)
Test.assert_equals(digital_root(942), 6)
Test.assert_equals(digital_root(132189), 6)
Test.assert_equals(digital_root(493193), 2)
