def find_uniq(arr):
    a = 0
    for i in arr[:2]:
        a += 1
        if i != arr[a] and i != arr[a + 1]:
            return i
    a = 0
    for i in arr:
        a += 1
        if i != arr[a]:
            return arr[a]

test.describe("Basic Tests")
test.assert_equals(find_uniq([ 1, 1, 1, 2, 1, 1 ]), 2)
test.assert_equals(find_uniq([ 0, 0, 0.55, 0, 0 ]), 0.55)
test.assert_equals(find_uniq([ 3, 10, 3, 3, 3 ]), 10)
