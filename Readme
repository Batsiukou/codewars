def anagrams(word, words):
    a = 0
    for i in words:
        for j in i:
            if j not in word:
                a = 0
            else:
                a += 1
            if a == len(word):
                print(i)

if __name__ == '__main__':

Test.assert_equals(anagrams('abba', ['aabb', 'abcd', 'bbaa', 'dada']), aabb bbaa)
Test.assert_equals(anagrams('racer', ['crazer', 'carer', 'racar', 'caers', 'racer']), carer racer)
